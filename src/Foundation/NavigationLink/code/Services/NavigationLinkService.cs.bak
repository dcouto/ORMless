using System;
using Foundation.BaseClasses.Services;
using Sitecore.Mvc.Presentation;

namespace Foundation.NavigationLink.Services
{
    public class NavigationLinkService : BaseService, INavigationLinkService
    {
        public NavigationLinkService(RenderingContext renderingContext, Guid expectedTemplateId) : base(renderingContext, expectedTemplateId)
        {
        }

        public Models.NavigationLink GetNavigationLink()
        {
            var model = new Models.NavigationLink(Datasource);

            return model;
        }
    }
}